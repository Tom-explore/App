{
  "dataconnect": {
    "source": "Back/dataconnect"
  },
  "functions": [
    {
      "source": "Back/functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run build"
      ]
    }
  ],
  "hosting": {
    "site": "tomexplore-c1c71",
    "public": "Front/build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "emulators": {
    "auth": { "port": 9099 },
    "functions": { "port": 5001 },
    "hosting": { "port": 5000 },
    "dataconnect": { "port": 9399 },
    "ui": { "enabled": true },
    "singleProjectMode": true
  }
}
