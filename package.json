{
  "name": "root",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run start --prefix Front",
    "emulators": "firebase emulators:start",
    "db:start": "docker-compose up -d",
    "build:functions": "npm run build --prefix functions",
    "serve": "concurrently \"npm run db:start\" \"npm run dev --prefix functions\" \"npm run emulators\" \"npm run dev\"",
    "build": "npm run build:front && npm run build:back",
    "build:front": "npm run build --prefix Front",
    "build:back": "npm run build --prefix functions",
    "start": "node Back/functions/lib/index.js"
  },

  "devDependencies": {
    "concurrently": "^7.0.0"
  },
  "dependencies": {
    "@firebase/app-check-interop-types": "^0.3.2",
    "@firebase/auth-interop-types": "^0.2.3",
    "@firebase/data-connect": "^0.1.1",
    "axios": "^1.7.7",
    "firebase": "^11.0.1"
  }
}

